{
  "monitor": {
    "interval": 60000,
    "numberOfBuilds": 30,
    "latestBuildOnly": false,
    "sortOrder": "date",
    "debug": true
  },
  "services": [
    {
      "name": "GitLab",
      "configuration": {
        "url": "https://git.jpg.com",
        "token": "TOKEN",
        "numberOfPipelinesPerProject": 30,
        "oneBuildPerBranch": true,
        "pipeline": {
          "status": ["running", "pending", "success", "failed", "canceled", "skipped", "manual"]
        },
        "slugs": [
          {
            "project": "prizmdoc/cells/predx"
          }
        ]
      }
    }
  ]
}